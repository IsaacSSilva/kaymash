{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "ESNext"],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "v1/*": ["./src/v1/src/hooks/*"],
      "option/v1/*": ["./src/v1/*"],
      "v0/*": ["./src/v0/src/*", "./src/v0/*"]
    }
  }
}
